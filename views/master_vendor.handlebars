<div style="margin-left: 200px; margin-right: 200px; font-size: 13px">

  <br>
  <h2>Vendor</h2>
  {{!-- <label class="heading">Owner History</label> --}}
  <br>
  <a href="/b_vendor_pages/create_b_vendor" style="font-size: 20px">Create</a>
  {{!-- <a href="/b_vendor_pages/create_b_vendor_add_div" style="font-size: 20px">Create add div</a> --}}
  <br><br>

  <div class="form-group" style="width: 1200px">
    <fieldset>
      <form action="" class="form-group">

        <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
          <thead>
            <tr>
              {{!-- <th scope="col" data-sortable="true">ID#</th> --}}
              <th scope="col">DIV</th>
              <th scope="col">VENDOR</th>
              <th scope="col">VENDOR NAME</th>
              <th scope="col">REGISTRATION NAME 1</th>
              <th scope="col">REGISTRATION NAME 2</th>
              <th scope="col">REGISTRATION ADDRESS</th>
              <th scope="col">LIEN HOLDER</th>
              {{!-- <th scope="col">Created</th>
                <th scope="col">Updated</th> --}}
              {{!-- <th scope="col">Delete</th> --}}
              <th scope="col">Delete</th>
              <th scope="col">Edit</th>
            </tr>
          </thead>

          <tbody>
            {{#each b_vendor}}
            <tr>
              {{!-- <td>{{this.id}}</td> --}}
              <td style="width:50px">{{this.vendor_div}}</td>
              <td style="width:50px">{{this.vendor_vendor_code}}</td>
              <td style="width:180px">{{this.vendor_vendor_name}}</td>
              <td style="width:220px">{{this.vendor_registration_name_1}}</td>
              <td style="width:220px">{{this.vendor_registration_name_2}}</td>
              <td style="width:250px">{{this.vendor_registration_address}}</td>
              <td style="width:180px">{{this.vendor_lien_holder}}</td>
              {{!-- <td style="width:170px">{{this.created_date}}</td>
                <td style="width:170px">{{this.updated_date}}</td> --}}
              {{!-- <td><button><a href="delete_b_division/{{this.id}}">Delete</a></button></td> --}}
              <td style="width:50px"><a href="/b_vendor_pages/delete_b_vendor/{{this.id}}">Delete</a></td>
              <td style="width:50px"><a href="/b_vendor_pages/edit_b_vendor/{{this.id}}">Edit</a></td>
            </tr>
            {{/each}}
          </tbody>

        </table>

      </form>
      <!--END OF FORM ^^-->
    </fieldset>

    <hr />
  </div>


